{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "memory": 2048,
        "maxDuration": 90
      }
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/image/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/chat/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/artworks/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/home/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/terms/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/special-requests/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/reports/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/transactions/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/categories/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/user/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/reviews/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/follow/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/notifications/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/dashboard/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/api/health",
      "dest": "/index.js"
    },
    {
      "src": "/api/db-test",
      "dest": "/index.js"
    },
    {
      "src": "/api/keepalive",
      "dest": "/index.js"
    },
    {
      "src": "/api/mongo-debug",
      "dest": "/index.js"
    },
    {
      "src": "/api/direct-connect",
      "dest": "/index.js"
    },
    {
      "src": "/api/api-routes",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]}
